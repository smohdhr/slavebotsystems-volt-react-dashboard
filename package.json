{
  "name": "@slavebotsystems/volt-react-dashboard",
  "description": "Volt React Dashboard is a free and open source admin dashboard template powered by React.js and Bootstrap 5",
  "author": "SlavebotSysterms",
  "type": "module",
  "exports": "./build/server.mjs",
  "browser": "./build/client.mjs",
  "keywords": [
    "bootstrap 5",
    "react",
    "react dashboard",
    "react admin template",
    "free react",
    "react ui library",
    "react bootstrap 5",
    "bootstrap 5 dashboard",
    "themesberg"
  ],
  "license": "MIT",
  "homepage": "http://localhost:3006/volt-react-dashboard",
  "bugs": {
    "url": "http://localhost:3006/volt-react-dashboard/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://localhost:3006/volt-react-dashboard/git"
  },
  "version": "1.0.1",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@themesberg/react-bootstrap": "^1.4.1",
    "bootstrap": "5.0.0-beta1",
    "chartist": "^0.11.4",
    "chartist-plugin-tooltips-updated": "^0.1.4",
    "moment-timezone": "^0.5.31",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-chartist": "^0.14.4",
    "react-copy-to-clipboard": "^5.0.3",
    "react-datetime": "^3.0.4",
    "react-dom": "^16.13.1",
    "react-github-btn": "^1.2.0",
    "react-live": "^2.2.3",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.3.1",
    "react-scripts": "3.4.3",
    "react-transition-group": "^4.4.1",
    "exenv": "1.2.2",
    "simplebar-react": "^2.3.0",
    "zlib": "1.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-local": "set PUBLIC_URL=./ & react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "dev:build-all": "set NODE_ENV=development & set PUBLIC_URL=public & webpack --config webpack.server.js --mode=development -w --config-name=server & webpack --config webpack.server.js --mode=development -w --config-name=client ",
    "dev:build-server": "set NODE_ENV=development & set PUBLIC_URL=public & webpack --config webpack.server.js --mode=development -w --config-name=server ",
    "dev:build-client": "set NODE_ENV=development & set PUBLIC_URL=public & webpack --config webpack.server.js --mode=development -w --config-name=client ",
    "dev:start": "set PUBLIC_URL=public & set SERVER_HOST=0.0.0.0 & set SERVER_PORT=3206 & node --experimental-modules ./build/server.mjs",
    "dev:startmon": "set PUBLIC_URL=public & set SERVER_HOST=0.0.0.0 & set SERVER_PORT=3206 & nodemon ./build/server.mjs",
    "dev": "npm-run-all --parallel build dev:*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "webpack": "5.88.2",
    "url-loader": "4.1.1",
    "babel-loader": "9.1.3",
    "@babel/preset-env": "7.22.15",
    "@babel/preset-react": "7.22.15",
    "sass-loader": "13.3.2",
    "sass": "^1.49.9",
    "webpack-node-externals": "3.0.0",
    "@themesberg/react-bootstrap": "^1.4.1",
    "copy-webpack-plugin": "11.0.0",
    "@compiled/webpack-loader": "0.11.1",
    "mini-css-extract-plugin": "2.7.6",
    "webpack-cli": "^5.1.4",
    "@svgr/webpack": "8.1.0"
  }
}
